{
  "name": "@fabrix/spool-broadcast",
  "version": "1.6.37",
  "description": "Spool: broadcast for Fabrix to implement CQRS and Event Sourcing",
  "homepage": "https://fabrix.app",
  "author": {
    "name": "scott-wyatt",
    "email": "scottwyatt86@gmail.com",
    "url": "https://cali-style.com"
  },
  "keywords": [
    "html",
    "broadcast",
    "CQRS",
    "event-sourcing",
    "rabbitMQ",
    "postgres",
    "sequelize",
    "spool",
    "fabrix",
    "trailjs"
  ],
  "scripts": {
    "release": "standard-version",
    "build": "tsc -p ./lib/tsconfig.release.json",
    "lint": "tslint -p ./lib",
    "watch": "tsc -w -p ./lib/tsconfig.release.json",
    "test": "npm run clean && npm run lint && npm run build && nyc mocha",
    "test:build:docs": "rimraf docs && typedoc --options typedoc.json lib/",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "ci": "cd .. && ci"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "@fabrix/regexdot": "^1.0.1",
    "@fabrix/spool-errors": "^1.6.2",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "primus-emitter": "^3.1.1",
    "primus-multiplex": "^3.2.1",
    "primus-resource": "^2.0.6",
    "primus-rooms": "^3.4.1",
    "rabbot": "^2.1.0"
  },
  "devDependencies": {
    "@fabrix/fabrix": "^1.6.4",
