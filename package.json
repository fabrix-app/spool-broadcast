    "@types/node": "~10.3.4",
    "express": "^4.17.1",
    "mocha": "^6",
    "nyc": "^14.1.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "should": "^13.2.3",
    "smokesignals": "^3.0.4",
    "source-map-support": "^0.5.13",
    "standard-version": "^7.0.0",
    "supertest": "^3.3.0",
    "tslib": "~1.9.0",
    "tslint": "~5.14.0",
    "tslint-microsoft-contrib": "~6.1.0",
    "tsutils": "~3.9.1",
    "typedoc": "^0.15.0",
